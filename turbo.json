{
	"$schema": "https://turbo.build/schema.json",
	"globalDependencies": ["**/.env.*local"],
	"globalEnv": ["*"],
	"tasks": {
		"build": {
			"dependsOn": ["^build"],
			"outputs": ["dist/**", ".next/**", "!.next/cache/**"]
		},
		"lint": {},
		"check-types": {
			"dependsOn": ["^check-types"]
		},
		"generate": {
			"cache": false
		},
		"dev": {
			"cache": false,
			"persistent": true,
			"dependsOn": [],
			"outputs": ["out/"]
		},
		"export": {
			"outputs": ["out/**"]
		}
	}
}
