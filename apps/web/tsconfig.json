{
	"extends": "@bbnocode/tsconfig/nextjs.json",
	"compilerOptions": {
		"plugins": [
			{
				"name": "next"
			}
		],
		"paths": {
			"@ui/*": ["./modules/ui/*"]
		},
		"noEmit": true
	},
	"include": [
		"**/*.ts",
		"**/*.tsx",
		"**/.cjs",
		"**/.mjs",
		".next/types/**/*.ts"
	],
	"exclude": ["node_modules"]
}
